{
  "name": "project-me",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "format:generated": "npx prettier --write ./src/generated/types/*",
    "generate": "npx type-crafter generate typescript-with-decoders ./docs/types/project.yaml ./src/generated/types && npm run format:generated",
    "dev": "vite dev --port 4366",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^6.4.17",
    "publint": "^0.2.0",
    "sass-embedded": "^1.83.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@types/marked": "^5.0.2",
    "chart.js": "^4.4.7",
    "dotenv": "^16.4.7",
    "lilac-typescript": "^1.3.1",
    "marked": "^15.0.6",
    "nodemailer": "^6.9.16",
    "type-decoder": "^2.0.1",
    "vergins": "3.6.8"
  }
}